---
import yaml from 'js-yaml'

import Default from '~/layouts/Default.astro'
import DataLoader from '~/components/DataLoader.astro'
import data2021Yml from '~/data/2021.yml?raw'

const data2021 = yaml.load(data2021Yml) as any

const title = '2021'
---

<Default
	title={title}
>

	<h1># 2021</h1>
	<p>Portal to projects useful for the 2021 year group</p>
	<p class="small text-light">
		<i class="fab fa-discord fa-fw"></i> = Discord server
	</p>

	<DataLoader data={data2021} />

</Default>
