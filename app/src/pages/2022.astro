---
import yaml from 'js-yaml'

import Default from '~/layouts/Default.astro'
import DataLoader from '~/components/DataLoader.astro'
import data2022Yml from '~/data/2022.yml?raw'

const data2022 = yaml.load(data2022Yml) as any

const title = '2022'
---

<Default
	title={title}
>

	<h1># 2022</h1>
	<p>Portal to projects useful for the 2022 year group</p>
	<p class="small text-light">
		<i class="fab fa-discord fa-fw"></i> = Discord server
	</p>

	<DataLoader data={data2022} />

</Default>
