---
import yaml from 'js-yaml'

import Default from '~/layouts/Default.astro'
import DataLoader from '~/components/DataLoader.astro'
import data2024Yml from '~/data/2024.yml?raw'

const data2024 = yaml.load(data2024Yml) as any

const title = '2024'
---

<Default
	title={title}
>

	<h1># 2024</h1>
	<p>Portal to projects useful for the 2024 year group</p>
	<p class="small text-light">
		<i class="fab fa-discord fa-fw"></i> = Discord server
	</p>

	<DataLoader data={data2024} />

</Default>
