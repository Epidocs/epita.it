---
import yaml from 'js-yaml'

import Default from '~/layouts/Default.astro'
import DataLoader from '~/components/DataLoader.astro'
import data2026Yml from '~/data/2026.yml?raw'

const data2026 = yaml.load(data2026Yml) as any

const title = '2026'
---

<Default
	title={title}
>

	<h1># 2026</h1>
	<p>Portal to projects useful for the 2026 year group</p>
	<p class="small text-light">
		<i class="fab fa-discord fa-fw"></i> = Discord server
	</p>

	<DataLoader data={data2026} />

</Default>
