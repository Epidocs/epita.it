---
import yaml from 'js-yaml'

import Default from '~/layouts/Default.astro'
import DataLoader from '~/components/DataLoader.astro'
import data2025Yml from '~/data/2025.yml?raw'

const data2025 = yaml.load(data2025Yml) as any

const title = '2025'
---

<Default
	title={title}
>

	<h1># 2025</h1>
	<p>Portal to projects useful for the 2025 year group</p>
	<p class="small text-light">
		<i class="icon icon-[fa-brands--discord] icon-fw"></i> = Discord server
	</p>

	<DataLoader data={data2025} />

</Default>
